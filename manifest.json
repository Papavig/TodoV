{
    "manifest_version": 2,
    "name": "Todo List",
    "version": "1.0",
    "description": "A simple todo list Chrome extension",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "permissions": [
      "storage"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    }
  }
  